{
  "name": "techsupport-pro",
  "version": "1.0.0",
  "description": "Sistema de gesti√≥n de tickets con SLA para TechSupport Pro",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "docker:dev:up": "docker-compose -f docker/docker-compose.dev.yml --env-file .env up -d",
    "docker:dev:down": "docker-compose -f docker/docker-compose.dev.yml --env-file .env down",
    "docker:dev:logs": "docker-compose -f docker/docker-compose.dev.yml --env-file .env logs -f",
    "docker:dev:reset": "docker-compose -f docker/docker-compose.dev.yml --env-file .env down -v",
    "docker:prod:up": "docker-compose -f docker/docker-compose.prod.yml --env-file .env.production up -d --build",
    "docker:prod:down": "docker-compose -f docker/docker-compose.prod.yml --env-file .env.production down",
    "docker:prod:logs": "docker-compose -f docker/docker-compose.prod.yml --env-file .env.production logs -f app",
    "docker:prod:reset": "docker-compose -f docker/docker-compose.prod.yml --env-file .env.production down -v"
  },
  "keywords": [
    "ticketing",
    "support",
    "sla",
    "nodejs",
    "typescript"
  ],
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "multer": "^2.0.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.1.0",
    "@types/uuid": "^10.0.0",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
